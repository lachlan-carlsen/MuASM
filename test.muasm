# Simple C major scale
main:
LDI R2, 261  # C4
LDI R3, 500  # 500ms duration
PLAYNOTE R2, R3

TRANSPOSE R2, 2  # D4
PLAYNOTE R2, R3

TRANSPOSE R2, 2  # E4
PLAYNOTE R2, R3

TRANSPOSE R2, 1  # F4
PLAYNOTE R2, R3

TRANSPOSE R2, 2  # G4
PLAYNOTE R2, R3

TRANSPOSE R2, 2  # A4
PLAYNOTE R2, R3

TRANSPOSE R2, 2  # B4
PLAYNOTE R2, R3

TRANSPOSE R2, 1  # C5
PLAYNOTE R2, R3
